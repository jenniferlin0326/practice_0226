doctype html
html(lang="zh-tw" data-git=htmlWebpackPlugin.options.git data-rev=htmlWebpackPlugin.options.rev)
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    title practice
  body
    .header
        .header-topBar
          .topBar-time.topBar-text 2018/03/09 17:14:59
          .topBar-menu
            a.topBar-text.lanBox.topBar-position 語言切換
              ul.lanList
                li.lanItem English
                li.lanItem 簡中
                li.lanItem 한국어
                li.lanItem 日本語
            a.topBar-text.userName.topBar-position Jennifer_lin
            a.topBar-text.userMoney.topBar-position BBIN 餘額： 0.24
        .header-main
          a.header-logo
          .header-version 傳統版
          .header-menu
            ul.menu-list
              li.menu-item
                a.menu-btn.menu-message
                ul.sec-menu-lst 公告
                  li.sec-menu-item
                    a.sec-menu-btn
              li.menu-item
                a.menu-btn.menu-function
                ul.sec-menu-lst 功能
                  li.sec-menu-item
                    a.sec-menu-btn
              li.menu-item
                a.menu-btn.menu-game
    .grid-mainContain
      .sidebar
        dl.sideBar-list
          dt.sideBar-title.sideBar-title-open 即時開獎
          dd.sideBar-detail 
            a.sideBar-btn BB PK3 <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail
            a.sideBar-btn 江蘇快3 <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail
            a.sideBar-btn BB 競速快樂彩 <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail
            a.sideBar-btn BB3D <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail 
            a.sideBar-btn BB 雷電PK <span class="sideBar-time">00:02:20</span>
        dl.sideBar-list  
          dt.sideBar-title.sideBar-title-hot 熱門遊戲
          dd.sideBar-detail 
            a.sideBar-btn BB PK3 <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail
            a.sideBar-btn 江蘇快3 <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail
            a.sideBar-btn BB 競速快樂彩 <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail
            a.sideBar-btn BB3D <span class="sideBar-time">00:02:20</span>
          dd.sideBar-detail 
            a.sideBar-btn BB 雷電PK <span class="sideBar-time">00:02:20</span>
        
      .container
        .tabBox
          .tab.tab-active <span class="tab-status tab-success"></span>Active <div class="close-bar"></div>
          .tab.tab-defult <span class="tab-status tab-disable"></span>Defult <div class="close-bar"></div>
          .tab.tab-defult <span class="tab-status tab-danger"></span>Defult <div class="close-bar"></div>
        .containBox
          .gameInfo-current
            .gameInfo-logo
            .gameInfoBox
              .gameInfo-name 重慶時時彩
              .gameInfo-num No.20180309-068
              .gameInfo-time 00:05:00
          .gameInfo-result
            .gameInfo-num No.20180309-067
            .gameInfo-resultBox
              span.resultNum.resultNum--L 4
              span.resultNum.resultNum--L 1
              span.resultNum.resultNum--L 4
              span.resultNum.resultNum--L 8
              span.resultNum.resultNum--L 7
          .gameInfo-history
            mixin game-history(num1,num2,num3,num4,num5)
              .gameInfo-num.hint-bottom
              .gameInfo-resultBox
                span.resultNum= num1
                span.resultNum= num2
                span.resultNum= num3
                span.resultNum= num4
                span.resultNum= num5
            +game-history('4','1','3','5','2')
            +game-history('7','4','0','6','3')
            +game-history('9','4','3','4','5')
            +game-history('8','1','3','6','4')
            +game-history('1','5','6','8','9')





